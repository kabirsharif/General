package practiceMakesPerfect;

public class Ranabhi2 {

	public static void main(String[] Args) {
		
		Ranabhi2 objct = new Ranabhi2();
		
		
		calculate(10, 5);
		sum(2, 4, 6);
		
		objct.divider(6, 2);
		objct.reminder(5, 2);
		
		
		
		
		
		
	}
	
	public static int calculate(int a, int b) {
		
		int result= a+b;
		System.out.println(result);
		return result;
	}
	
	public static int sum(int a, int b, int c) {
		
		return a+b+c;
	}
	public float divider(float a, float b) {
	
	return a/b;
}
	public double reminder(double a, double b) {
	
	return a%b;
}
}
